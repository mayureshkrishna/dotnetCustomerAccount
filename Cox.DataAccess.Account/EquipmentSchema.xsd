<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="EquipmentSchema" targetNamespace="http://tempuri.org/EquipmentSchema.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/EquipmentSchema.xsd" xmlns:mstns="http://tempuri.org/EquipmentSchema.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="EquipmentSchema" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="ActivePpvEvents">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SERIAL_NUMBER" type="xs:string" minOccurs="0" />
							<xs:element name="EVENT_TITLE" type="xs:string" minOccurs="0" />
							<xs:element name="EVENT_START_DATE" type="xs:decimal" minOccurs="0" />
							<xs:element name="EVENT_START_TIME" type="xs:decimal" minOccurs="0" />
							<xs:element name="END_DATE" type="xs:decimal" minOccurs="0" />
							<xs:element name="END_TIME" type="xs:decimal" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CustomerEquipment">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="SERIAL_NUMBER" type="xs:string" minOccurs="0" />
							<xs:element name="ITEM_NUMBER" type="xs:string" minOccurs="0" />
							<xs:element name="PORT_TYPE" type="xs:string" minOccurs="0" />
							<xs:element name="EQUIPMENT_ADDRESS" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>