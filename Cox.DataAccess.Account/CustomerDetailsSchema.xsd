<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="CustomerDetailsSchema" targetNamespace="http://tempuri.org/CustomerDetailsSchema.xsd"
	elementFormDefault="qualified" attributeFormDefault="qualified" xmlns="http://tempuri.org/CustomerDetailsSchema.xsd"
	xmlns:mstns="http://tempuri.org/CustomerDetailsSchema.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="CustomerDetailsSchema" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="CustomerHSIData">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ACCOUNT_NUMBER" type="xs:int" minOccurs="0" />
							<xs:element name="SITE_ID" type="xs:int" minOccurs="0" />
							<xs:element name="HSD_USER_LOGIN_ID" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CustomerPlanData">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="PLN_ACCOUNT" type="xs:int" minOccurs="0" />
							<xs:element name="SITE_ID" type="xs:int" minOccurs="0" />
							<xs:element name="PLN_ASSIGNMENT_EXP_DATE" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CustomerTelephoneData">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ACCOUNT_NUMBER" type="xs:int" minOccurs="0" />
							<xs:element name="SITE_ID" type="xs:int" minOccurs="0" />
							<xs:element name="TN_CHARACTER_STRING" type="xs:string" minOccurs="0" />
							<xs:element name="toll_carrier" type="xs:string" minOccurs="0" />
							<xs:element name="intra_lata_carrier" type="xs:string" minOccurs="0" />
							<xs:element name="CNAM_pres_ind" type="xs:string" minOccurs="0" />
							<xs:element name="CUSTOMER_TN_TYPE_ID" type="xs:string" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="CustomerCommentsData">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ACCOUNT_NUMBER" type="xs:int" minOccurs="0" />
							<xs:element name="SITE_ID" type="xs:int" minOccurs="0" />
							<xs:element name="COMMENT_ENTER_DATE" type="xs:int" minOccurs="0" />
							<xs:element name="COMMENT_LINE" type="xs:string" minOccurs="0" />
							<xs:element name="USER_ID" type="xs:string" minOccurs="0" />
							<xs:element name="SEQUENCE_NUMBER" type="xs:int" minOccurs="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>